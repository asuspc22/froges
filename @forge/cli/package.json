{
  "name": "@forge/cli",
  "version": "6.14.0",
  "description": "A command line interface for managing Atlassian-hosted apps",
  "author": "Atlassian",
  "license": "UNLICENSED",
  "bin": {
    "forge": "out/bin/cli.js"
  },
  "homepage": "https://developer.atlassian.com/platform/forge",
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./out && rm -f tsconfig.tsbuildinfo",
    "compile": "tsc -b -v",
    "dev": "NODE_ENV=development ts-node src/bin/cli.ts",
    "inspect": "node -r ts-node/register --inspect-brk src/bin/cli.ts",
    "postbuild": "chmod +x out/bin/*.js && ts-node scripts/configureAutocomplete.ts",
    "postinstall": "node -e \"process.exitCode = fs.existsSync('./out/bin/postinstall.js')\" || node ./out/bin/postinstall.js"
  },
  "dependencies": {
    "@forge/bundler": "4.10.1",
    "@forge/cli-shared": "3.15.0",
    "@forge/egress": "1.2.1",
    "@forge/lint": "3.6.0",
    "@forge/manifest": "4.16.0",
    "@forge/runtime": "4.4.2",
    "@forge/tunnel": "3.6.1",
    "@forge/util": "1.2.3",
    "@sentry/node": "^7.29.0",
    "ajv": "^6.12.6",
    "archiver": "^5.2.0",
    "case": "^1.6.3",
    "chalk": "^2.4.2",
    "cheerio": "^0.22.0",
    "cli-table3": "^0.6.3",
    "command-exists": "^1.2.9",
    "commander": "^7.0.0",
    "cross-spawn": "^7.0.3",
    "dayjs": "^1.9.7",
    "didyoumean": "^1.2.2",
    "env-paths": "^2.2.1",
    "form-data": "^3.0.1",
    "fs-extra": "^8.1.0",
    "hidefile": "^3.0.0",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "node-fetch": "2.6.11",
    "node-machine-id": "^1.1.12",
    "omelette": "^0.4.17",
    "ora": "^4.0.3",
    "portfinder": "^1.0.32",
    "sanitize-filename": "^1.6.3",
    "semver": "^6.3.0",
    "tmp": "^0.1.0",
    "tslib": "^2.4.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/cheerio": "^0.22.31",
    "@types/command-exists": "^1.2.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/didyoumean": "^1.2.0",
    "@types/fs-extra": "^8.0.1",
    "@types/hidefile": "^3.0.0",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.1.2",
    "@types/js-yaml": "^3.12.2",
    "@types/minimatch": "^3.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "14.18.51",
    "@types/node-fetch": "^2.6.4",
    "@types/omelette": "^0.4.2",
    "@types/semver": "^6.0.1",
    "@types/tmp": "^0.1.0",
    "@types/uuid": "^3.4.10",
    "jest": "^29.1.2",
    "jest-fixtures": "^0.6.0",
    "jest-matcher-specific-error": "^1.0.0",
    "jest-when": "^3.5.2",
    "memfs": "^3.4.1",
    "sentry-testkit": "^5.0.5",
    "type-fest": "1.4.0"
  },
  "engines": {
    "node": ">=12.13.1"
  }
